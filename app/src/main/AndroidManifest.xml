<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.demo.newvpn">
<!--    android:dataExtractionRules="@xml/data_extraction_rules"-->
<!--    android:fullBackupContent="@xml/backup_rules"-->
    <application
        android:name=".util.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewVpn"
        android:roundIcon="@mipmap/ic_launcher_round">
        <activity
            android:name=".ac.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.demo.newvpn.ac.HomeAc"
            android:screenOrientation="portrait"
            android:launchMode="singleTask"/>
        <activity android:name="com.demo.newvpn.ac.ServerListAc"
            android:screenOrientation="portrait"/>
        <activity android:name="com.demo.newvpn.ac.ResultAc"
            android:screenOrientation="portrait"/>
        <activity android:name="com.demo.newvpn.ac.WebAc"
            android:screenOrientation="portrait"/>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_id" />
    </application>

</manifest>